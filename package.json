{
    "scripts": {
        "create_db": "ts-node ./server/migrations/create-db.ts",
        "create_table_users": "ts-node ./server/migrations/create-table/1.create-table-users.ts",
        "migration_all": "ts-node ./server/migrations/migration-all-table.ts",
        "api-docs": "swagger-cli bundle ./server/docs/openapi.yaml --outfile server/docs/swagger.yaml",
        "server": "cd server && nodemon --watch \"./**/*.ts\" --exec \"ts-node\" ./server.ts",
        "client": "cd client && yarn start",
        "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
    },
    "dependencies": {
        "axios": "^0.27.2",
        "concurrently": "^7.2.1",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.0.1",
        "googleapis": "^100.0.0",
        "jsonwebtoken": "^8.5.1",
        "mysql2": "^2.3.3",
        "nodemailer": "^6.7.5",
        "regex-email": "^1.0.2",
        "sequelize-typescript": "^2.1.3",
        "swagger-cli": "^4.0.4",
        "swagger-ui-express": "^4.4.0",
        "util": "^0.12.4",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.39",
        "@types/nodemailer": "^6.4.4",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/validator": "^13.7.2",
        "@types/yamljs": "^0.2.31",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "child_process": "^1.0.2",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.30.0",
        "express": "^4.18.1",
        "nodemon": "^2.0.16",
        "prettier": "^2.6.2",
        "ts-node": "^10.8.0",
        "typescript": "^4.7.2"
    }
}
